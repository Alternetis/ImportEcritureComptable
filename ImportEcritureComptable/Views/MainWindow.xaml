<Window x:Class="ImportEcritureComptable.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ImportEcritureComptable"
        mc:Ignorable="d"
        Title="Import Ecriture Comptable" Height="250" Width="450"
        ResizeMode="NoResize" WindowStartupLocation="CenterScreen" WindowStyle="SingleBorderWindow" Background="White">
    <Grid Margin="10">
        <Grid.RowDefinitions>
            <RowDefinition  Height="2*"></RowDefinition>
            <RowDefinition ></RowDefinition>
        </Grid.RowDefinitions>
        <Frame Grid.Column="0" Grid.Row="0" x:Name="frameMain" NavigationUIVisibility="Hidden" />
        <Button Grid.Row="2" Content="Import vers SAGE" Name="ImportBtn" IsEnabled="False"
                FontSize="15" FontWeight="Bold" Margin="5" 
                Background="AliceBlue" BorderThickness="2" Click="ImportBtn_Click"></Button>
    </Grid>
</Window>
